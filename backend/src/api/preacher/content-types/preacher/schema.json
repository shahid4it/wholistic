{
  "kind": "collectionType",
  "collectionName": "preachers",
  "info": {
    "singularName": "preacher",
    "pluralName": "preachers",
    "displayName": "Readers & Healers"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "bio": {
      "type": "text"
    },
    "profile": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "tags": {
      "type": "string"
    },
    "tools": {
      "type": "string"
    },
    "oneliner": {
      "type": "string"
    },
    "topics": {
      "type": "string"
    },
    "abilities": {
      "type": "string"
    },
    "style": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "name",
      "required": true
    },
    "services": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::service.service",
      "mappedBy": "preacher"
    },
    "rating": {
      "type": "decimal",
      "default": 4.3
    },
    "specialty": {
      "type": "enumeration",
      "enum": [
        "reader",
        "healer"
      ]
    }
  }
}
