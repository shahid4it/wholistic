{
  "kind": "collectionType",
  "collectionName": "horoscopes",
  "info": {
    "singularName": "horoscope",
    "pluralName": "horoscopes",
    "displayName": "Horoscope"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "starName": {
      "type": "enumeration",
      "enum": [
        "Aries",
        "Taurus",
        "Gemini",
        "Cancer",
        "Leo",
        "Virgo",
        "Libra",
        "Scorpio",
        "Sagittarius",
        "Capricorn",
        "Aquarius",
        "Pisces"
      ]
    },
    "title": {
      "type": "string"
    },
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "publishDate": {
      "type": "date"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
