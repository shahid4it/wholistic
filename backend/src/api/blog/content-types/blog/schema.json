{
  "kind": "collectionType",
  "collectionName": "blogs",
  "info": {
    "singularName": "blog",
    "pluralName": "blogs",
    "displayName": "Resources"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "content": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "summary": {
      "type": "text"
    },
    "author": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::preacher.preacher"
    },
    "publishDate": {
      "type": "date"
    },
    "tags": {
      "type": "string"
    },
    "category": {
      "type": "enumeration",
      "default": "blog",
      "enum": [
        "blog",
        "video",
        "podcast",
        "online-classes"
      ]
    },
    "resource": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "resourceUrl": {
      "type": "text"
    }
  }
}
